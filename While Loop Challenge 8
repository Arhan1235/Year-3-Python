import random
number = random.randint(1,10)
attempts = 0
guess = ""


#add a While loop here
while guess != number and attempts < 5:
    guess=int(input("Guess a number"))
    attempts = attempts + 1
    if attempts == 5:
      print("You're out of attempts")
if guess == number:
   print("Yay! Correct guess")

